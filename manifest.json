{
    "manifest_version": 2,

    "name": "toggle-quiet-mode",
    "description": "Toggle quiet-mode-chrome",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "contentSettings",
        "notifications",
        "https://ajax.googleapis.com/"
    ]
}
